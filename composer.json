{
    "name": "koala-framework/kwf-trl",
    "description": "Translation scripts for Koala Framework based applications",
    "require": {
        "koala-framework/kwf-trl-jsparser": "1.1.x-dev",
        "symfony/console": "~2.6",
        "symfony/finder": "~2.6",
        "sepia/po-parser": "~4.2",
        "nikic/php-parser": "~1.0",
        "gitonomy/gitlib": "~0.1",
        "twig/twig": "~1.16",
        "psr/log": "~1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "3.7.*"
    },
    "license": "BSD-2-Clause",
    "autoload": {
        "psr-0": { "Kwf\\Trl\\": "" }
    },
    "autoload-dev": {
        "psr-0": {
            "Kwf_Ext_": "tests"
        }
    },
    "bin": ["bin/trl"],
    "minimum-stability": "dev",
    "prefer-stable": true
}
