{
    "name": "koala-framework/kwf-trl",
    "description": "Translation scripts for Koala Framework based applications",
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/Ben-Ho/PHP-po-parser.git"
        }
    ],
    "require": {
        "koala-framework/kwf-trl-jsparser": "dev-master",
        "symfony/console": "~2.6",
        "symfony/finder": "~2.6",
        "sepia/po-parser": "dev-master as 4.2.1",
        "nikic/php-parser": "1.0.2"
    },
    "require-dev": {
        "phpunit/phpunit": "3.7.*"
    },
    "extra": {
        "require-npm": {
            "esprima": "1.2.2"
        }
    },
    "license": "BSD-2-Clause",
    "autoload": {
        "psr-0": { "Kwf\\Trl\\": "" }
    },
    "autoload-dev": {
        "psr-0": {
            "Kwf_Ext_": "tests"
        }
    },
    "bin": ["bin/trl"],
    "minimum-stability": "dev",
    "prefer-stable": true
}
